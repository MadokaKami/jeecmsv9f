<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jeecms.cms.entity.assist">
	<class name="CmsAcquisitionReplace" table="jc_acquisition_replace">
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id name="id" type="java.lang.Integer" column="replace_id"><generator class="identity"/></id>
		<property name="keyword" column="keyword" type="string" not-null="false" length="255"/>
		<property name="replaceWord" column="replace_word" type="string" not-null="false" length="255"/>
		<many-to-one name="acquisition" column="acquisition_id" class="com.jeecms.cms.entity.assist.CmsAcquisition" not-null="true"></many-to-one>
	</class>	
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.core.entity">
	<class
		name="CmsOss"
		table="jc_oss"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="ossAppId"
			column="app_id"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="secretId"
			column="secret_id"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="appKey"
			column="app_key"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="bucketName"
			column="bucket_name"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="bucketArea"
			column="bucket_area"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="endPoint"
			column="end_point"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="accessDomain"
			column="access_domain"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="ossType"
			column="oss_type"
			type="java.lang.Byte"
			not-null="true"
			length="2"
		/>
		<property
			name="name"
			column="oss_name"
			type="string"
			not-null="true"
			length="255"
		/>


	</class>	
</hibernate-mapping>